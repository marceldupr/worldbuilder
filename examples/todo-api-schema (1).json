    {
      "id": "c1752d66-db99-46ad-bd96-33a2b3568cd5",
      "type": "enforcer",
      "name": "TaskValidationEnforcer",
      "description": "Enforcer for Task",
      "schema": {
        "rules": [
          {
            "name": "Task Title Required",
            "type": "validation",
            "trigger": "before_create",
            "condition": "task.title !== undefined && task.title.trim() !== ''",
            "components": [
              "Task"
            ],
            "description": "Ensures that a task cannot be created without a title.",
            "errorMessage": "Task title is required."
          },
          {
            "name": "Task Due Date Valid",
            "type": "validation",
            "trigger": "before_create",
            "condition": "task.dueDate === undefined || task.dueDate > new Date()",
            "components": [
              "Task"
            ],
            "description": "Validates that a task's due date is set to a future date.",
            "errorMessage": "Task due date must be in the future."
          },
          {
            "name": "Task Assignment Permission",
            "type": "permission",
            "trigger": "before_update",
            "condition": "user.role === 'admin' || task.assigneeId === user.id",
            "components": [
              "Task",
              "User"
            ],
            "description": "Checks if the user has permission to assign or change the assignee of a task.",
            "errorMessage": "You do not have permission to assign this task."
          }
        ]
      },
      "status": "ready",
      "locked": false
    },
    {
      "id": "ed2e20a5-c648-42bf-bc6b-cf02dd7f8dff",
      "type": "worker",
      "name": "CleanupWorker",
      "description": "Worker for Task",
      "schema": {
        "worker": {
          "queue": "cleanup_tasks",
          "retry": {
            "backoff": "exponential",
            "attempts": 3
          },
          "steps": [
            {
              "name": "validate_task",
              "type": "async"
            },
            {
              "name": "process_cleanup",
              "type": "async"
            },
            {
              "name": "finalize_cleanup",
              "type": "async"
            }
          ],
          "helpers": [
            "task_validator",
            "cleanup_processor",
            "notification_sender"
          ],
          "timeout": 30000,
          "concurrency": 5
        }
      },
      "status": "ready",
      "locked": false
    },
