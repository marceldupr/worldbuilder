    {
      "id": "696b8546-4c18-4bfd-9c31-f4176e5f41dd",
      "type": "workflow",
      "name": "TaskUpdateWorkflow",
      "description": "Workflow for Task",
      "schema": {
        "steps": [
          {
            "name": "RetrieveTask",
            "action": "getTaskDetails",
            "onError": "retry",
            "timeout": 30,
            "component": "Task",
            "description": "Fetch the task details that need to be updated."
          },
          {
            "name": "UpdateTask",
            "action": "updateTask",
            "onError": "abort",
            "timeout": 60,
            "component": "Task",
            "description": "Update the task with new information."
          },
          {
            "name": "NotifyUser",
            "action": "sendNotification",
            "onError": "skip",
            "component": "Task",
            "description": "Notify the user about the task update status."
          }
        ],
        "trigger": "http",
        "rollback": {
          "steps": [
            {
              "name": "RevertTask",
              "action": "revertTask",
              "component": "Task",
              "description": "Revert the task back to its previous state."
            }
          ],
          "enabled": true
        },
        "errorHandling": {
          "notFound": "Notify user that the task was not found.",
          "updateFailed": "Log the error and notify support team.",
          "validationError": "Return validation errors to the user."
        }
      },
      "status": "ready",
      "locked": false
    },
